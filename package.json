{
  "name": "react-starter",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && NODE_ENV=production webpack --progress --bail --env dist -p --config ./webpack.prd.config.babel.js",
    "dev": "./node_modules/.bin/webpack-dev-server --mode development --progress --config ./webpack.dev.config.babel.js --hot --history-api-fallback",
    "test:unit": "mocha --growl",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:snapshot": "jest --config ./test/jest.config.json",
    "test:snapshot:watch": "npm run test:snapshot -- --watch"
  },
  "dependencies": {
    "babel-eslint": "8.2.5",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-babel": "5.1.0",
    "history": "3.2.1",
    "html-webpack-plugin": "3.2.0",
    "immutable": "3.8.2",
    "node-sass": "4.9.3",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-hot-loader": "4.3.3",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "5.0.7",
    "react-router": "3.2.0",
    "react-router-redux": "4.0.8",
    "redux": "4.0.0",
    "redux-immutablejs": "0.0.8",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-css-modules-transform": "1.6.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "^4.1.2",
    "css-loader": "0.28.11",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "5.4.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "growl": "^1.10.5",
    "ignore-styles": "^5.0.1",
    "jest": "^23.5.0",
    "jest-context": "^2.1.0",
    "jsdom": "^12.0.0",
    "mocha": "^5.2.0",
    "react-test-renderer": "^16.4.2",
    "sass-loader": "7.0.3",
    "sinon": "^6.1.5",
    "style-loader": "0.21.0",
    "webpack-dev-server": "3.1.6"
  }
}
